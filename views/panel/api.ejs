<%- include('../includes/manage'); -%>
<main class="h-full pb-16 overflow-y-auto">
    <br>
    <h1 style="text-align:center; font-size: 40px;">Klucz API</h1>
    <div class="flex">
        <div class="flex-1" style="margin: 40px">
          <div class="card text-center shadow-2xl">
              <div class="card-body">
                  <div class="overflow-x-auto ">
                    <form  class="card-body" action="/panel/manage/<%= result[0].id %>/api/generate" method="POST">
                        <div class="w-full mb-2">
                            <div class="form-control">
                                <input type="text" placeholder="Klucz API" class="input input-bordered" value="<%= result[0].apikey %>" name="key" disabled>
                            </div>
                        </div>
                        <div class="card-actions">
                            <button type="submit" class="btn btn-wide">Generuj nowy</button>
                        </div>
                    </form>
                  </div>
              </div>
          </div> 
        </div>
    </div>
</main>
<%- include('../includes/footer'); -%>